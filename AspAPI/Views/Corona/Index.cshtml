
@{
    ViewBag.Title = "Data Corona";
}

<!-- Container Fluid -->
<div class="container-fluid">
    <!-- Bread crumb and right sidebar toggle -->
    <div class="row page-titles">
        <div class="col-md-6 col-8 align-self-center">
            <h3 class="text-themecolor m-b-0 m-t-0">@ViewBag.title</h3>
            <ol class="breadcrumb">
                <li class="breadcrumb-item active">@ViewBag.title</li>
            </ol>
        </div>
        @*<div class="col-md-6 col-4 align-self-center">
                <a onclick="PopupForm('@Url.Action("Create","Supplier")')" class="btn pull-right hidden-sm-down btn-success text-white">
                    <i class="mdi mdi-plus-circle"></i> Add Data
                </a>
            </div>*@
    </div>
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- Start Page Content -->
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    @*<h4 class="card-title">Data Export</h4>
                        <h6 class="card-subtitle">You can Export data to Soft Copy, CSV, Excel, PDF & Print</h6>*@
                    <div class="table-responsive m-t-40">
                        <table id="DataTable1" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Country</th>
                                    <th>Cases</th>
                                    <th>Deaths</th>
                                    <th>Recovered</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody class="BodyTable">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Page Content -->

</div>
<!-- End Container fluid  -->

